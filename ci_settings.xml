<settings>
  <servers>
    <server>
      <id>gitlab-maven</id>
      <configuration>
        <httpHeaders>
          <property>
            <name>Job-Token</name>
            <value>${env.CI_JOB_TOKEN}</value>
          </property>
        </httpHeaders>
      </configuration>
    </server>
    <server>
      <id>ossrh</id>
      <username>${sonatype_jira_id}</username>
      <password>${sonatype_jira_pwd}</password>
    </server>    
  </servers>
</settings>
